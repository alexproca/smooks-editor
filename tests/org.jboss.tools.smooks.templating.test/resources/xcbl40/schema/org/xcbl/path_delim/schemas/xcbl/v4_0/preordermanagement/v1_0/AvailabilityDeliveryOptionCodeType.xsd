    <xsd:simpleType name="AvailabilityDeliveryOptionCodeType">
        <xsd:annotation>
            <xsd:documentation>This code identifies the type of delivery required for the <!--code-->AvailabilityToPromise<!--/code-->.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:NMTOKEN">
            <xsd:enumeration value="Other">
                <xsd:annotation>
                    <xsd:documentation>Custom code</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Complete">
                <xsd:annotation>
                    <xsd:documentation>The delivery is required as a complete shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Partial">
                <xsd:annotation>
                    <xsd:documentation>The delivery will be accepted as a partial shipment</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="OneTime">
                <xsd:annotation>
                    <xsd:documentation>the delivery is considered to be a single event.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
</xsd:schema>
