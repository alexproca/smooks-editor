    <xsd:complexType name="AvailabilityCheckResultType">
        <xsd:annotation>
            <xsd:documentation>contains the resulting
     response to an <!--code-->AvailabilityCheckRequest<!--/code--> for specific supplier items.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AvailabilityCheckResultHeader" type="AvailabilityCheckResultHeaderType">
                <xsd:annotation>
                    <xsd:documentation>holds reference dates, supplier and buyer information, and any special
      notes associated with the <!--code-->AvailabilityCheckResult<!--/code-->.
        This element occurs once within the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ListOfAvailabilityCheckResultItemDetail" type="ListOfAvailabilityCheckResultItemDetailType">
                <xsd:annotation>
                    <xsd:documentation>contains a list of line items for the <!--code-->AvailabilityCheckResult<!--/code-->.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
