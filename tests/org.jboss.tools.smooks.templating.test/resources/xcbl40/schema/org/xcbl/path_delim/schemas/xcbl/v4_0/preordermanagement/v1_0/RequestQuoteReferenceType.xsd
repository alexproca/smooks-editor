    <xsd:complexType name="RequestQuoteReferenceType">
        <xsd:annotation>
            <xsd:documentation>contains a list of references for the request.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ContractReference" type="core:ContractType">
                <xsd:annotation>
                    <xsd:documentation>identifies the contract number which the request is referencing.
        </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AccountNumber" type="core:ReferenceType">
                <xsd:annotation>
                    <xsd:documentation>contains the buyers account code with the supplier.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PriceListNumber" type="core:ReferenceType">
                <xsd:annotation>
                    <xsd:documentation>identifies the reference number of the price list.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PriceListVersionNumber" type="core:ReferenceType">
                <xsd:annotation>
                    <xsd:documentation>identifies the version number of the price list.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BuyersCatalogNumber" type="core:ReferenceType">
                <xsd:annotation>
                    <xsd:documentation>identifies the catalog number of the buyer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="OtherRequestQuoteReferences" type="core:ListOfReferenceCodedType">
                <xsd:annotation>
                    <xsd:documentation>lists any other references to the Request not documented by the
        <!--code-->ContractReference<!--/code-->. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
