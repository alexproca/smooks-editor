    <xsd:complexType name="AvailabilityToPromiseItemDetailType">
        <xsd:annotation>
            <xsd:documentation>contains the line item detail information.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AvailabilityToPromiseBaseItemDetail" type="AvailabilityToPromiseBaseItemDetailType">
                <xsd:annotation>
                    <xsd:documentation>contains the identifying and descriptive information for the line item in the
<!--code-->AvailabilityToPromise<!--/code-->. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AvailabilityToPromiseDeliveryDetail" type="core:DeliveryDetailType">
                <xsd:annotation>
                    <xsd:documentation>identifies the delivery details for the line item in the
        <!--code-->AvailabilityToPromise<!--/code-->. </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AvailabilityToPromiseTransportDetail" type="core:TransportRoutingType">
                <xsd:annotation>
                    <xsd:documentation>identifies the transport details for the line item in the
        <!--code-->AvailabilityToPromise<!--/code-->.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AvailabilityToPromiseItemListOfAttachment" type="core:ListOfAttachmentType">
                <xsd:annotation>
                    <xsd:documentation>contains attachments for the line item.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LineItemNote" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation>contains any free-form text pertinent to the line item in
					<!--code-->AvailabilityToPromise<!--/code-->. This element may contain notes or any other similar information that
					is not contained explicitly in another structure. it should not be assumed that the receiving application is capable of
					doing more than storing and/or displaying this information.
					</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ListOfAvailabilityToPromiseItemDetailType">
        <xsd:annotation>
            <xsd:documentation>is the container for the list of line items.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="AvailabilityToPromiseItemDetail" type="AvailabilityToPromiseItemDetailType">
                <xsd:annotation>
                    <xsd:documentation>contains the line item detail information for <!--code-->AvailabilityToPromise<!--/code-->.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
